    </li>


  <!-- 
    =================================
      NAVIGATION
    =================================
   -->

    <li data-row="3" data-col="7" data-sizex="1" data-sizey="1">
      <div data-id="cpageupstairs" data-view="Changepage" data-icon="ellipsis-h" data-title="Lights" data-pageid="page_lights" data-stagger="false" data-fasttransition="true"></div>
    </li>

    <li data-row="4" data-col="7" data-sizex="1" data-sizey="1">
      <div data-id="cpagebasement" data-view="Changepage" data-icon="ellipsis-h" data-title="Basement" data-pageid="page_basement" data-stagger="false" data-fasttransition="true"></div>
    </li>

    <li data-row="5" data-col="7" data-sizex="1" data-sizey="1">
      <div data-id="cpagesecurity" data-view="Changepage" data-icon="ellipsis-h" data-title="Doors" data-pageid="page_doors" data-stagger="false" data-fasttransition="true"></div>
    </li>

